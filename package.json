    ]
  },
  "dependencies": {
    "expo": "~36.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-jest": "^25.1.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-expo": "^36.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  },
  "private": true
